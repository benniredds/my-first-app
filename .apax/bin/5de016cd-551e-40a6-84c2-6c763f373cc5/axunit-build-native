#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../packages/@ax/axunit/axunit-build-native-runner.js" "$@"
else
  exec node  "$basedir/../../packages/@ax/axunit/axunit-build-native-runner.js" "$@"
fi
