<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="test-cases-meta">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="test-cases">
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="0" maxOccurs="unbounded" name="test-case">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element minOccurs="0" maxOccurs="1" name="source-location">
                      <xs:complexType>
                        <xs:attribute name="file" type="xs:string" use="required" />
                        <xs:attribute name="line" type="xs:unsignedInt" use="required" />
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="id" type="xs:unsignedInt" use="required" />
                  <xs:attribute name="name" type="xs:string" use="required" />
                  <xs:attribute name="result-variable" type="xs:string" use="required" />
                  <xs:attribute name="parameter" type="xs:string" use="required" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="name" type="xs:string" use="required" />
          </xs:complexType>
        </xs:element>
        <xs:element name="assert-locations">
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="0" maxOccurs="unbounded" name="location">
                <xs:complexType>
                  <xs:attribute name="id" type="xs:unsignedInt" use="required" />
                  <xs:attribute name="source-name" type="xs:string" use="required" />
                  <xs:attribute name="start-column" type="xs:unsignedInt" use="required" />
                  <xs:attribute name="end-column" type="xs:unsignedInt" use="required" />
                  <xs:attribute name="start-line" type="xs:unsignedInt" use="required" />
                  <xs:attribute name="end-line" type="xs:unsignedInt" use="required" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="variable-name" type="xs:string" use="required" />
            <xs:attribute name="array-size" type="xs:unsignedInt" use="required" />
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>